# Test 1 SingleCustomerInSingleMonthWithMultipleTransactions

# Expect -  [ { "customerId": 1001, "yearMonth": 2020-01, "points": 91 } ]

[{
     "customerId": 1001,
     "transDate": "2020-01-30",
     "transAmount": 1.34 
   }, 
   {
     "customerId": 1001,
     "transDate": "2020-01-12",
     "transAmount": 51.34 
   },
   {
     "customerId": 1001,
     "transDate": "2020-01-10",
     "transAmount": 120.00 
   },
   {
     "customerId": 2,
     "transDate": "2020-01-30",
     "transAmount": 59.34 
   }  
]

# Test 2 MultipleCustomerInSingleMonthWithMultipleTransactions

Expect -  [ { "customerId": 1001, "yearMonth": 2020-02, "points": 52 }, 
{ "customerId": 1002, "yearMonth": 2020-02, "points": 5 } ]

[{
     "customerId": 1001,
     "transDate": "2020-02-26",
     "transAmount": 49.00 
   }, 
   {
     "customerId": 1001,
     "transDate": "2020-02-13",
     "transAmount": 101.34 
   },
   {
     "customerId": 1002,
     "transDate": "2020-02-10",
     "transAmount": 16.99 
   },
   {
     "customerId": 1002,
     "transDate": "2020-02-03",
     "transAmount": 11.92 
   },
   {
     "customerId": 1002,
     "transDate": "2020-02-23",
     "transAmount": 55.34 
   }  
]

# Test 3 SingleCustomerInMultipleMonthWithMultipleTransactions

Expect -  [ { "customerId": 1001, "yearMonth": 2020-02, "points": 140 }, 
{ "customerId": 1001, "yearMonth": 2020-03, "points": 1 } ]

[{
     "customerId": 1001,
     "transDate": "2020-02-26",
     "transAmount": 100.00 
   }, 
   {
     "customerId": 1001,
     "transDate": "2020-02-13",
     "transAmount": 120.34 
   },
   {
     "customerId": 1001,
     "transDate": "2020-03-10",
     "transAmount": 51.99 
   },
   {
     "customerId": 1001,
     "transDate": "2020-03-12",
     "transAmount": 11.92 
   },
   {
     "customerId": 1001,
     "transDate": "2020-02-23",
     "transAmount": 15.34 
   }  
]


